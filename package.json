{
  "name": "vs-deploy",
  "displayName": "Deploy",
  "description": "Commands for deploying files of your workspace to a destination.",
  "version": "2.0.4",
  "publisher": "mkloubert",
  "engines": {
    "vscode": "^1.5.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "keywords": [
    "Deploy",
    "Workspace",
    "Files",
    "FTP",
    "SSH"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "menus": {
      "editor/title": [
        {
          "command": "extension.deploy.file",
          "title": "Deploy this file"
        }
      ],
      "editor/context": [
        {
          "command": "extension.deploy.file",
          "title": "Deploy this file"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.deploy",
        "title": "Workspace",
        "category": "Deploy"
      },
      {
        "command": "extension.deploy.file",
        "title": "Current file",
        "category": "Deploy"
      },
      {
        "command": "extension.deploy.listen",
        "title": "Start/stop listening for files",
        "category": "Deploy"
      }
    ],
    "keybindings": [
      {
        "command": "extension.deploy",
        "key": "ctrl+alt+w",
        "mac": "cmd+alt+w"
      },
      {
        "command": "extension.deploy.file",
        "key": "ctrl+alt+f",
        "mac": "cmd+alt+f"
      },
      {
        "command": "extension.deploy.listen",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "icon": "icon.png",
  "author": {
    "name": "Marcel Joachim Kloubert"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mkloubert/vs-deploy"
  },
  "bugs": {
    "url": "https://github.com/mkloubert/vs-deploy/issues"
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "fs-extra": "^1.0.0",
    "ftp": "^0.3.10",
    "glob": "^7.1.1",
    "moment": "^2.17.1",
    "node-zip": "^1.1.1",
    "nodemailer": "^2.7.0",
    "opn": "^4.0.2",
    "ssh2-sftp-client": "^1.0.5",
    "zip": "^1.2.0"
  }
}
